<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginRight="40dp"
    android:layout_marginLeft="40dp"
    android:orientation="vertical"
    tools:context="mx.edu.ittepic.dadm_u2_practica7_anacarolinamondragonrangel.ArregloComoParametro">

   <TextView
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="ESCRIBA N VALORES ENTEROS SEPARADOS POR COMAS"
       android:layout_marginTop="20dp"
       android:textColor="#000000"
       android:textSize="20dp"/>
   <EditText
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:id="@+id/edittxt"/>
   <Button
       android:id="@+id/btnabrir"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="ABRIR OTRO ACTIVITY"
       android:layout_marginTop="20dp"/>

</LinearLayout>
